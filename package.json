{
  "name": "e2d",
  "description": "A declarative canvas renderer.",
  "main": "dist/e2d.js",
  "version": "4.0.0-rc.3",
  "scripts": {
    "build": "rollup -c",
    "build-all": "npm run build && npm run minify && npm run compat && npm run compat-min",
    "compat": "babel dist/e2d.js > dist/e2d.compat.js",
    "compat-min": "minify dist/e2d.compat.js > dist/e2d.compat.min.js",
    "dev-server": "run-p watch sync-test-folder web-server",
    "pretty": "prettier --write \"src/*.js\"",
    "minify": "minify dist/e2d.js > dist/e2d.min.js",
    "prepublish": "npm run pretty && npm run build-all && git add --all . && git commit -am \"Version 4.0.0-rc.3\" && git push origin master --tags",
    "sync-test-folder": "run-s copy-e2d-test watch-e2d-test",
    "copy-e2d-test": "cat ./dist/e2d.js > ./public/e2d.js",
    "watch-e2d-test": "watch -p \"dist/e2d.js\" -c \"npm run copy-e2d-test\"",
    "watch": "npm run watch-substep && watch -p \"src/*.js\" -c \"npm run watch-substep\"",
    "watch-compat": "watch -p \"dist/e2d.js\" -c \"npm run compat\"",
    "watch-compat-min": "watch -p \"dist/e2d.compat.js\" -c \"npm run compat-min\"",
    "watch-substep": "npm run build && npm run minify && npm run pretty",
    "web-server": "http-server -d false -c-1"
  },
  "author": "Joshua Tenner",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-minify": "^0.2.0",
    "babel-preset-env": "^1.6.1",
    "babel-runtime": "^6.26.0",
    "http-server": "^0.10.0",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.8.2",
    "rollup": "^0.51.6",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "sync-glob": "^1.3.7",
    "watch-cli": "^0.2.3"
  },
  "dependencies": {
    "keycode": "^2.1.9",
    "point-in-polygon": "^1.0.1"
  },
  "bundledDependencies": [
    "keycode",
    "point-in-polygon"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/jtenner/e2d.git"
  },
  "keywords": [
    "canvas",
    "2d",
    "declarative",
    "graphics",
    "browser"
  ]
}
