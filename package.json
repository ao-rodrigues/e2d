{
  "name": "e2d",
  "description": "A declarative canvas renderer.",
  "main": "dist/e2d.js",
  "version": "4.0.0-rc.2",
  "scripts": {
    "build": "node node_modules/rollup/bin/rollup -c",
    "watch": "npm run build && npm run minify && node node_modules/watch-cli/bin/watch -p \"src/*.js\" -c \"npm run build && npm run minify\"",
    "dev-server": "npm run watch | npm run sync-test | npm run web-server",
    "minify": "node node_modules/babel-minify/bin/minify dist/e2d.js > dist/e2d.min.js",
    "sync-test": "node node_modules/sync-glob/bin/sync-glob.js -w 'dist/e2d.js' 'test/*' public/",
    "web-server": "http-server -d false -c-1"
  },
  "author": "Joshua Tenner",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-minify": "^0.2.0",
    "babel-preset-env": "^1.6.0",
    "babel-runtime": "^6.26.0",
    "rollup": "^0.50.0",
    "rollup-plugin-commonjs": "^8.2.1",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "sync-glob": "^1.3.7",
    "watch-cli": "^0.2.2"
  },
  "dependencies": {
    "keycode": "^2.1.9",
    "point-in-polygon": "^1.0.1"
  },
  "bundledDependencies": [
    "keycode",
    "point-in-polygon"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/jtenner/e2d.git"
  },
  "keywords": [
    "canvas",
    "2d",
    "declarative",
    "graphics",
    "browser"
  ]
}
